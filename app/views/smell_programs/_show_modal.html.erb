  <!-- Modal -->
  <div class="modal fade" id="<%=locals[:id]%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle"><% locals[:category] %></h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <% @scents[locals[:category]].each do |scent|%>
            <%= scent.name %> <br>
          <% end %>

          <p>sdfsdfsdfdsf</p>
          <div class="scrolling-wrapper">
            <% current_user.scents_to_program(@scents[locals[:category]]).each do |program| %>
            <% raise %>
            <%= render "sliding_card", locals: {training_id: program.id}  %>
            <% end %>
          </div>
          <p>sdfsfsdfsdf</p>

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          <%= link_to "Save Changes", smell_program_path, class: "btn btn-primary" %>
        </div>
      </div>
    </div>
  </div>
